import sqlite3
db = sqlite3.connect('school.db')
c = db.cursor()
c.execute('INSERT INTO course(code, name) VALUES(?,?)', ('code', 'name'))
c.execute('SELECT * FROM course')
        records = c.fetchall()
        print(records)
c.execute('DROP TABLE course')



